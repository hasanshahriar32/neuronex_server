{
  "version": 2,

  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/generate/(.*)",
      "methods": ["POST"],
      "dest": "index.js",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ],

  "functions": {
    "api/generate/**/*.js": {
      "maxDuration": 30
    }
  },

  "edge": {
    "defaultConfig": "your-vercel-edge-config-file.js"
  }
}

{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/generate/(.*)",
      "methods": ["POST"],
      "dest": "index.js",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ],
  "functions": {
    "generateResponse": {
      "maxDuration": 30
    }
  },
  "edge": {
    "defaultConfig": "your-vercel-edge-config-file.js"
  }
}
